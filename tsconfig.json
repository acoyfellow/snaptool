{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "jsxFactory": "jsx",
    "jsxFragmentFactory": "Fragment",
    "types": ["@cloudflare/workers-types", "./src/env.d.ts"],
    "noEmit": true
  },
  "include": ["src", "src_new"]
}